{
  "name": "@local-loop/localloop-backend",
  "version": "0.2.0",
  "private": true,
  "description": "Backend API for LocalLoop expression-of-interest submissions",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=25.1.0"
  },
  "volta": {
    "node": "25.1.0"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "start": "bun run src/index.ts",
    "migrate": "bun run src/db/migrate.ts",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "import:sqlite": "bun run scripts/import-sqlite.ts",
    "seed:lab": "bun run scripts/seed-lab.ts",
    "simulate:lab": "bun run scripts/simulate-lab.ts",
    "lab:demo": "bun run scripts/lab-demo.ts",
    "simulate:federation": "bun run scripts/simulate-federation.ts",
    "lab:federation": "bun run scripts/lab-federation.ts",
    "prisma:generate": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^7.3.0",
    "@prisma/adapter-pg": "^7.3.0",
    "@aws-sdk/client-s3": "^3.980.0",
    "@fastify/cors": "^11.2.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.6.1",
    "better-auth": "^1.4.18",
    "bullmq": "^5.67.2",
    "fastify": "^5.7.2",
    "ioredis": "^5.9.2",
    "pg": "^8.18.0",
    "prisma": "^7.3.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.1.0",
    "@types/pg": "^8.16.0",
    "bun-types": "^1.3.8",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "fast-xml-parser": "5.3.4",
    "hono": "4.11.7",
    "lodash": "4.17.23"
  }
}
